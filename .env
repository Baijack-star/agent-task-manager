# 环境配置
NODE_ENV=production

# 服务器端口配置
PORT=3000

# 文件路径配置（指向实际工作目录）
# 修复配置不一致问题：指向24houragent根目录的inbox.md和outbox.md
INBOX_PATH=D:\\WorkSpace\\24houragent\\inbox.md
OUTBOX_PATH=D:\\WorkSpace\\24houragent\\outbox.md

# 跨域配置
CORS_ORIGIN=*

# 日志级别
LOG_LEVEL=info

# WebSocket配置
WS_HEARTBEAT_INTERVAL=30000

# 安全配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
